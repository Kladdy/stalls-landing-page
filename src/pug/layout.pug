//- layout.pug
doctype html
html(lang='sv')
    head
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
        meta(name='description', content='DESCRIPTION')
        //- meta(name='author', content='')

        title Stall Stj√§rnholm - #{title}

        // Favicon
        link(rel='icon', type='image/x-icon', href='assets/favicon.ico')

        // Bootstrap icons
        link(href='https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.0/font/bootstrap-icons.css', rel='stylesheet', type='text/css')

        // FontAwesome JS
        script(src="https://kit.fontawesome.com/d859ded2d4.js" crossorigin="anonymous")

        // Google fonts
        link(href='https://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic,700italic', rel='stylesheet', type='text/css')

        // Core theme CSS (includes Bootstrap)
        link(href='css/styles.css', rel='stylesheet')

        // jquery
        script(src="https://code.jquery.com/jquery-2.1.4.min.js")

        // Custom scripts
        script(src="js/scripts.js")

        // navbar JS
        script(src='js/navbar.js')

        block scripts
            
            
    body(style="background-color: #212529")
        include navbar

        block content

        include footer 
        //- block footer
        //-     #footer
        //-         p some footer content

        // Bootstrap core JS
        script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js')

        // Core theme JS
        script(src='js/scripts.js')